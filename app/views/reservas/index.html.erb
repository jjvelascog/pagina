<h1>Reservas</h1>
<Table>
	<tr>
		<th>Cliente</th>
		<th>SKU</th>
		<th>Cantidad</th>
		<th>Fecha</th>
	</tr>
<% Reserva.all.each do |reserva| %>
	<tr>
		<td><%= reserva.cliente %></td>
		<td><%= reserva.sku %></td>
		<td><%= reserva.cantidad %></td>
		<td><%= reserva.fecha %></td>
	</tr>
<% end %>
<% end %>
